<h1>Edit <%= @pantry.name %></h1>

<form action="/pantries/<%=@pantry.id %>" method="POST">
  <label> Pantry Name </label>
  <input type="text" name="name" value="<%= @pantry.name %>"> </br>

  <label> Nationality </label>
  <input type="text" name="nationality" value="<%= @pantry.nationality %>"> </br>

  <input type="submit" value="Update">
       
</form>
